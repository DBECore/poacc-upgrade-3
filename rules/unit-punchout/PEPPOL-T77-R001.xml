<?xml version="1.0" encoding="UTF-8"?>
<!--BII3-T77-R010-->
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-T77-base-3.0">
	<assert>
		<description>Verify shopping cart lines quantities are greater than zero</description>
		<scope>PEPPOL-T77-R001</scope>
	</assert>
	
	<test>
		<assert>
			<success>PEPPOL-T77-R001</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:CatalogueLine>
				<cac:RequiredItemLocationQuantity>
					<cac:Price>
						<cbc:BaseQuantity unitCode="C62">1</cbc:BaseQuantity>
					</cac:Price>
					<cac:DeliveryUnit>
						<cbc:BatchQuantity unitCode="C62">1</cbc:BatchQuantity>
					</cac:DeliveryUnit>
				</cac:RequiredItemLocationQuantity>
			</cac:CatalogueLine>

		</Catalogue>
	</test>

	<test>
		<assert>
			<error number="1">PEPPOL-T77-R001</error>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:CatalogueLine>
				<cac:RequiredItemLocationQuantity>
					<cac:Price>
						<cbc:BaseQuantity unitCode="C62">0</cbc:BaseQuantity>
					</cac:Price>
					<cac:DeliveryUnit>
						<cbc:BatchQuantity unitCode="C62">0</cbc:BatchQuantity>
					</cac:DeliveryUnit>
				</cac:RequiredItemLocationQuantity>
			</cac:CatalogueLine>

		</Catalogue>
	</test>

	<test>
		<assert>
			<error number="2">PEPPOL-T77-R001</error>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:CatalogueLine>
				<cac:RequiredItemLocationQuantity>
					<cac:Price>
						<cbc:BaseQuantity unitCode="C62">-1</cbc:BaseQuantity>
					</cac:Price>
					<cac:DeliveryUnit>
						<cbc:BatchQuantity unitCode="C62">-1</cbc:BatchQuantity>
					</cac:DeliveryUnit>
				</cac:RequiredItemLocationQuantity>
			</cac:CatalogueLine>

		</Catalogue>
	</test>
	
</testSet>