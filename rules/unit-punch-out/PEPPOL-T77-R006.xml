<?xml version="1.0" encoding="UTF-8"?>
<!--BII3-T77-R024-->
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-t77-base-3.0">
	<assert>
		<description>Verify that an item have at most one manufacturer item identification</description>
		<scope>PEPPOL-T77-R006</scope>
	</assert>
	
	<test>
		<assert>
			<success>PEPPOL-T77-R006</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:CatalogueLine>
				<cac:Item>
					<cac:ManufacturersItemIdentification>
						<cbc:ID>MNTR01349087911</cbc:ID>
					</cac:ManufacturersItemIdentification>
				</cac:Item>
			</cac:CatalogueLine>
		</Catalogue>
	</test>
	
	<test>
		<assert>
			<success>PEPPOL-T77-R006</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:CatalogueLine>
				<cac:Item>
				</cac:Item>
			</cac:CatalogueLine>
		</Catalogue>
	</test>
	
	<test>
		<assert>
			<warning number="1">PEPPOL-T77-R006</warning>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:CatalogueLine>
				<cac:Item>
					<cac:ManufacturersItemIdentification>
						<cbc:ID>MNTR01349087911</cbc:ID>
					</cac:ManufacturersItemIdentification>
					<cac:ManufacturersItemIdentification>
						<cbc:ID>MNTR01349087912</cbc:ID>
					</cac:ManufacturersItemIdentification>
				</cac:Item>
			</cac:CatalogueLine>

		</Catalogue>
	</test>
	
</testSet>