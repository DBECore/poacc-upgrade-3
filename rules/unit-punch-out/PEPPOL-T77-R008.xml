<?xml version="1.0" encoding="UTF-8"?>
<!--BII3-T77-R026-->
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-T77-base-3.0">
	<assert>
		<description>Verify that an item have at most one attachment document description</description>
		<scope>PEPPOL-T77-R008</scope>
	</assert>
	
	<test>
		<assert>
			<success>PEPPOL-T77-R008</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:CatalogueLine>
				<cac:Item>
					<cac:ItemSpecificationDocumentReference>
						<cbc:DocumentDescription>text</cbc:DocumentDescription>
					</cac:ItemSpecificationDocumentReference>
				</cac:Item>
			</cac:CatalogueLine>
		</Catalogue>
	</test>
	
	<test>
		<assert>
			<success>PEPPOL-T77-R008</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:CatalogueLine>
				<cac:Item>
					<cac:ItemSpecificationDocumentReference>
					</cac:ItemSpecificationDocumentReference>
				</cac:Item>
			</cac:CatalogueLine>
		</Catalogue>
	</test>
	
	<test>
		<assert>
			<warning number="1">PEPPOL-T77-R008</warning>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:CatalogueLine>
				<cac:Item>
					<cac:ItemSpecificationDocumentReference>
						<cbc:DocumentDescription>text1</cbc:DocumentDescription>
						<cbc:DocumentDescription>text2</cbc:DocumentDescription>
					</cac:ItemSpecificationDocumentReference>
				</cac:Item>
			</cac:CatalogueLine>
		</Catalogue>
	</test>
	
</testSet>