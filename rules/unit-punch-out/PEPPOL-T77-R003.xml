<?xml version="1.0" encoding="UTF-8"?>
<!--BII3-T77-R012-->
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-T77-base-3.0">
	<assert>
		<description>Verify that either “item sellers identifier” or “item standard identifier” is specified</description>
		<scope>PEPPOL-T77-R003</scope>
	</assert>
	
	<test>
		<assert>
			<success>PEPPOL-T77-R003</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:CatalogueLine>
				<cac:Item>
					<cac:SellersItemIdentification>
						<cbc:ID>MNTR011</cbc:ID>
					</cac:SellersItemIdentification>
					<cac:StandardItemIdentification>
						<cbc:ID schemeID="0160">1234567890114</cbc:ID>
					</cac:StandardItemIdentification>
				</cac:Item>
			</cac:CatalogueLine>

		</Catalogue>
	</test>
	
	<test>
		<assert>
			<success>PEPPOL-T77-R003</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:CatalogueLine>
				<cac:Item>
					<cac:SellersItemIdentification>
						<cbc:ID>MNTR011</cbc:ID>
					</cac:SellersItemIdentification>
				</cac:Item>
			</cac:CatalogueLine>

		</Catalogue>
	</test>
	
	<test>
		<assert>
			<success>PEPPOL-T77-R003</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:CatalogueLine>
				<cac:Item>
					<cac:StandardItemIdentification>
						<cbc:ID schemeID="0160">1234567890114</cbc:ID>
					</cac:StandardItemIdentification>
				</cac:Item>
			</cac:CatalogueLine>

		</Catalogue>
	</test>

	<test>
		<assert>
			<error number="1">PEPPOL-T77-R003</error>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:CatalogueLine>
				<cac:Item>
				</cac:Item>
			</cac:CatalogueLine>

		</Catalogue>
	</test>
	
	<test>
		<assert>
			<error number="2">PEPPOL-T77-R003</error>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:CatalogueLine>
				<cac:Item>
					<cac:SellersItemIdentification>
						<cbc:ID></cbc:ID>
					</cac:SellersItemIdentification>
					<cac:StandardItemIdentification>
						<cbc:ID schemeID="0160"></cbc:ID>
					</cac:StandardItemIdentification>
				</cac:Item>
			</cac:CatalogueLine>

		</Catalogue>
	</test>
	
</testSet>