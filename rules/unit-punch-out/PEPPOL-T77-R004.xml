<?xml version="1.0" encoding="UTF-8"?>
<!--BII3-T77-R017-->
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-T77-base-3.0">
	<assert>
		<description>Verify that validity period end date is specified</description>
		<scope>PEPPOL-T77-R004</scope>
	</assert>
	
	<test>
		<assert>
			<success>PEPPOL-T77-R004</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:ValidityPeriod>
				<cbc:EndDate>2016-08-31</cbc:EndDate>
			</cac:ValidityPeriod>

		</Catalogue>
	</test>
	
	<test>
		<assert>
			<error number="1">PEPPOL-T77-R004</error>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:ValidityPeriod>
				<cbc:EndDate></cbc:EndDate>
			</cac:ValidityPeriod>

		</Catalogue>
	</test>
	
	<test>
		<assert>
			<error number="2">PEPPOL-T77-R004</error>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:ValidityPeriod>
			</cac:ValidityPeriod>

		</Catalogue>
	</test>
</testSet>