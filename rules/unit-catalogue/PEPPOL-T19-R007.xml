<?xml version="1.0" encoding="UTF-8"?>
<!--BII2-T19-R017-->
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-t19-base-3.0">
	<assert>
		<description>Verify line validity period is within document validity period</description>
		<scope>PEPPOL-T19-R007</scope>
	</assert>
	
	<test>
		<assert>
			<success>PEPPOL-T19-R007</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:ValidityPeriod>
				<cbc:StartDate>2018-09-01</cbc:StartDate>
				<cbc:EndDate>2019-08-31</cbc:EndDate>
			</cac:ValidityPeriod>
			
			<cac:CatalogueLine>
				<cac:LineValidityPeriod>
					<cbc:StartDate>2018-09-01</cbc:StartDate>
					<cbc:EndDate>2019-08-31</cbc:EndDate>
				</cac:LineValidityPeriod>
			</cac:CatalogueLine>
		</Catalogue>
	</test>
	
	<test>
		<assert>
			<success>PEPPOL-T19-R007</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:ValidityPeriod>
				<cbc:StartDate>2018-09-01</cbc:StartDate>
				<cbc:EndDate>2019-08-31</cbc:EndDate>
			</cac:ValidityPeriod>
			
			<cac:CatalogueLine>
				<cac:LineValidityPeriod>
					<cbc:StartDate>2018-09-10</cbc:StartDate>
					<cbc:EndDate>2019-08-10</cbc:EndDate>
				</cac:LineValidityPeriod>
			</cac:CatalogueLine>
		</Catalogue>
	</test>

	<test>
		<assert>
			<success>PEPPOL-T19-R007</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:ValidityPeriod>
				<cbc:StartDate>2018-09-01</cbc:StartDate>
				<cbc:EndDate>2019-08-31</cbc:EndDate>
			</cac:ValidityPeriod>
			<!-- not sure about the validity of this-->
			<cac:CatalogueLine>
				<cac:LineValidityPeriod>
					<cbc:StartDate>2018-09-10</cbc:StartDate>
				</cac:LineValidityPeriod>
			</cac:CatalogueLine>
		</Catalogue>
	</test>

	<test>
		<assert>
			<success>PEPPOL-T19-R007</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:ValidityPeriod>
				<cbc:StartDate>2018-09-01</cbc:StartDate>
				<cbc:EndDate>2019-08-31</cbc:EndDate>
			</cac:ValidityPeriod>
			
			<cac:CatalogueLine>
				<cac:LineValidityPeriod>
				</cac:LineValidityPeriod>
			</cac:CatalogueLine>
		</Catalogue>
	</test>
	
	<test>
		<assert>
			<warning number="1">PEPPOL-T19-R007</warning>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:ValidityPeriod>
				<cbc:StartDate>2018-09-01</cbc:StartDate>
				<cbc:EndDate>2019-08-31</cbc:EndDate>
			</cac:ValidityPeriod>
			
			<cac:CatalogueLine>
				<cac:LineValidityPeriod>
					<cbc:StartDate>2018-08-10</cbc:StartDate>
					<cbc:EndDate>2019-08-10</cbc:EndDate>
				</cac:LineValidityPeriod>
			</cac:CatalogueLine>
		</Catalogue>
	</test>
	<test>
		<assert>
			<warning number="2">PEPPOL-T19-R007</warning>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
