<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-t16-base-3.0">
	<assert>
		<description>Verify each line has a name or item identifier</description>
		<scope>PEPPOL-T16-R003</scope>
	</assert>
	<test>
		<assert>
			<success>PEPPOL-T16-R003</success>
		</assert>
		<DespatchAdvice xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2" xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:DespatchLine>
				<cbc:ID>1</cbc:ID>
				<cac:Item>
					<cbc:Name>Item123</cbc:Name>
					<cac:SellersItemIdentification>
						<cbc:ID>010120401</cbc:ID>
					</cac:SellersItemIdentification>
					<cac:StandardItemIdentification>
						<cbc:ID schemeID="GTIN" schemeAgencyID="9" schemeAgencyName="GS1">7611104117056</cbc:ID>
					</cac:StandardItemIdentification>
				</cac:Item>
			</cac:DespatchLine>
		</DespatchAdvice>
	</test>

	
	<test>
		<assert>
			<success>PEPPOL-T16-R003</success>
		</assert>
		<DespatchAdvice xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2" xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:DespatchLine>
				<cbc:ID>1</cbc:ID>
				<cac:Item>
					<cac:SellersItemIdentification>
						<cbc:ID>010120401</cbc:ID>
					</cac:SellersItemIdentification>
				</cac:Item>
			</cac:DespatchLine>
		</DespatchAdvice>
	</test>
	
	<test>
		<assert>
			<success>PEPPOL-T16-R003</success>
		</assert>
		<DespatchAdvice xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2" xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:DespatchLine>
				<cbc:ID>1</cbc:ID>
				<cac:Item>
					<cac:StandardItemIdentification>
						<cbc:ID schemeID="GTIN" schemeAgencyID="9" schemeAgencyName="GS1">7611104117056</cbc:ID>
					</cac:StandardItemIdentification>
				</cac:Item>
			</cac:DespatchLine>
		</DespatchAdvice>
	</test>
	
	<test>
		<assert>
			<success>PEPPOL-T16-R003</success>
		</assert>
		<DespatchAdvice xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2" xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:DespatchLine>
				<cbc:ID>1</cbc:ID>
				<cac:Item>
					<cbc:Name>Item123</cbc:Name>
				</cac:Item>
			</cac:DespatchLine>
		</DespatchAdvice>
	</test>
	
	<test>
		<assert>
			<error>PEPPOL-T16-R003</error>
		</assert>
		<DespatchAdvice xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2" xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:DespatchLine>
				<cbc:ID>1</cbc:ID>
				<cac:Item>
				</cac:Item>
			</cac:DespatchLine>
		</DespatchAdvice>
	</test>

</testSet>
